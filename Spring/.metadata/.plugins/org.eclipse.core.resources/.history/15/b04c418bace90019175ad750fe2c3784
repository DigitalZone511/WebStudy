package sample05;

import org.springframework.context.annotation.ComponentScan;

import lombok.Data;

@ComponentScan("com.conf")
@Data
public class SungJukDTO2 {
	private String name;
	private int kor;
	private int eng;
	private int math;
	private int tot;
	private int avg;
	
	@Override
	public String toString() {
		return name+"\t"+kor+"\t"+eng+"\t"+math+"\t"+tot+"\t"+String.format("%.3f", avg);
	}
}
